{
  "name": "vices-backend",
  "version": "1.0.0",
  "description": "Backend API for Vices app with Google Maps integration",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "lint": "eslint **/*.ts",
